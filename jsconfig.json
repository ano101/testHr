{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["resources/js/*"],
      "@composables/*": ["resources/js/composables/*"],
      "@components/*": ["resources/js/Components/*"]
    },
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "preserve",
    "allowJs": true,
    "checkJs": false
  },
  "include": [
    "resources/js/**/*"
  ],
  "exclude": [
    "node_modules",
    "vendor",
    "public",
    "storage"
  ]
}
